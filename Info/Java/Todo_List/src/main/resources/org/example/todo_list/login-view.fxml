<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.control.PasswordField?>
<AnchorPane prefHeight="75.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.todo_list.LoginController">
   <children>
      <HBox alignment="CENTER" prefHeight="75.0" prefWidth="400.0">
         <children>
            <PasswordField prefWidth="200.0" fx:id="loginTextField">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin>
            </PasswordField>
            <Button mnemonicParsing="false" text="Login" fx:id="loginButton" onAction="#onLogin">
               <opaqueInsets>
                  <Insets left="20.0" />
               </opaqueInsets>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
